<mvc:View controllerName="com.flexible.flexible.controller.Third" xmlns="sap.m" xmlns:f="sap.f" xmlns:l="sap.ui.layout"
	xmlns:mvc="sap.ui.core.mvc" xmlns:core="sap.ui.core" xmlns:smartTable="sap.ui.comp.smarttable" xmlns:smartField="sap.ui.comp.smartfield"
	xmlns:smartForm="sap.ui.comp.smartform">
	<f:DynamicPage headerExpanded="false" id="thirdPage">
		<f:title>
			<f:DynamicPageTitle>
				<f:heading>
					<Title text="Third Page"/>
				</f:heading>
				<f:actions>
					<Button icon="sap-icon://full-screen" type="Transparent"
						visible="{= ${flexibleColumnLayoutConfig>/actionButtonsInfo/endColumn/fullScreen} !== null }" press="handleFullScreen"/>
					<Button icon="sap-icon://exit-full-screen" type="Transparent"
						visible="{= ${flexibleColumnLayoutConfig>/actionButtonsInfo/endColumn/exitFullScreen} !== null }" press="handleExitFullScreen"/>
					<Button icon="sap-icon://decline" type="Transparent"
						visible="{= ${flexibleColumnLayoutConfig>/actionButtonsInfo/endColumn/closeColumn} !== null }" press="handleClose"/>
				</f:actions>
			</f:DynamicPageTitle>
		</f:title>
	</f:DynamicPage>
</mvc:View>